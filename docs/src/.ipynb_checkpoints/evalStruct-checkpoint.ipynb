    "function evalStruct(self::Struct)\n",
    "\tdim = checkStruct(self.body)\n",
    "   \tCTM, stack = Matrix{Float64}(LinearAlgebra.I, dim+1, dim+1), []\n",
    "   \tscene = traversal(CTM, stack, self, [])\n",
    "\treturn scene\n",
    "end"
